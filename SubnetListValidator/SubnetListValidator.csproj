<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <OutputType>Exe</OutputType>
        <TargetFramework>net6.0-windows</TargetFramework>
        <RuntimeIdentifier>win-x64</RuntimeIdentifier>
        <PublishSingleFile>true</PublishSingleFile>
        <SelfContained>true</SelfContained>
        <PublishReadyToRun>true</PublishReadyToRun>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>enable</Nullable>
        <ApplicationIcon>app-icon.ico</ApplicationIcon>
        <Copyright>Copyright Â© 2022 Yegor Pelykh</Copyright>
    </PropertyGroup>

    <ItemGroup>
      <Content Include="app-icon.ico" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="IPNetwork2" Version="2.6.467" />
        <PackageReference Include="WhoisClient.NET" Version="4.0.0" />
    </ItemGroup>

</Project>
